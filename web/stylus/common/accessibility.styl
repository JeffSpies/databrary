// styles

.accessible-hidden
  position absolute
  left -10000px
  top auto
  width 1px
  height 1px
  overflow hidden

:focus
  padding 5px
  background-color #fff0f2
  outline #1919ff auto 5px !important
  outline -webkit-focus-ring-color auto 5px !important
  & *
    background-color #fff0f2

input:focus
  background-color #fff0f2 !important

#map_wrapper
  background-color transparent !important
  & *
    background-color transparent !important