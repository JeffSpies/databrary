# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET     /                      		controllers.Application.start
GET     /login                      	controllers.Application.login
POST    /Login                 		controllers.Application.postLogin
GET     /OpenID                     	controllers.Application.openID(username = "")
GET     /OpenID/:username           	controllers.Application.openID(username : String)
GET	/logout				controllers.Application.logout
GET	/home			   	controllers.Application.home
POST	/Account			controllers.Application.postAccount
POST	/Authorize			controllers.Application.postAuthorize

GET     /DDL                        	controllers.Application.ddl

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               	controllers.Assets.at(path="/public", file)
