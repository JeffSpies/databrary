.input-completer-choices
  background-color white
  border 1px black solid
  z-index 9
  max-height 200px
  overflow-y scroll
  position absolute
  width inherit
  padding-right 1

.input-completer-choice
  margin-bottom f8

  &:last-child
    margin-bottom 0

  a
    border-left 1px solid gray
    border-right 1px solid gray
    border-bottom 1px solid gray
    border-top 1px solid pale
    display block
    padding f4 f2

    &:hover
      background-color yellow51
      color blue21
