// styles

form fieldset
  border-bottom-width 0
  padding-bottom 5px
  padding-top 5px
  .drop-zone
     border 2px solid black
     &.over-form
     	border 4px solid black
     &.over-zone
     	border 4px solid green
  .file-thumb
     height 50px
     width 50px
  .trash
     border 2px solid black

.fix-bottom
  &.row
    position fixed
    bottom 200px
    left 60px
