// styles

.cf
	clearfix()

// big stuff

*
*:after
*:before
	box-sizing border-box

	&:focus
		outline 0

html
	overflow-x hidden !important

body
	font-line 1
	color black
	background white

// bottom-margin

.foot
	margin-bottom b1

.foot-2
	margin-bottom b2

.foot-cut
	*:last-child
		margin-bottom 0
